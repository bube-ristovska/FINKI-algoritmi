package Listi;

public class SLLNode {
    protected int data;
    SLLNode link;

    public SLLNode(int data, SLLNode link) {
        this.data = data;
        this.link = link;
    }

    @Override
    public String toString() {
        return "SLLNode " +
                "jazol=" + data + " ";
    }
}
