package AIPS;

public class SLLNode <E>{
    protected E data;
    protected SLLNode successor;

    public SLLNode(E data, SLLNode successor) {
        this.data = data;
        this.successor = successor;
    }

    @Override
    public String toString() {
        return "SLLNode{" +
                "data=" + data +
                ", successor=" + successor +
                '}';
    }
}
